    "browserify": "^14.0.0",
    "chokidar-cli": "^1.2.0",
    "nodemon": "^1.11.0",
    "standard": "^9.0.0-beta.0"
  },
  "dependencies": {
    "@dasilvacontin/keyboard": "^1.0.2",
    "express": "^4.14.1",
